        Title="MainWindow" Height="350" Width="525">

    <Window.DataContext>
        <vm:MainViewModel />
    </Window.DataContext>

    <Grid>
        <Label Content="{Binding laps}" HorizontalAlignment="Left" Margin="10,86,0,0" VerticalAlignment="Top" Height="118" Width="497" FontSize="72"/>
    </Grid>
</Window>
